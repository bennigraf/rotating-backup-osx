<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>de.bennigraf.backuprotation</string>
		<key>ProgramArguments</key>
		<array>
			<string>/usr/bin/php</string>
			<string>/path/to/backuprotation.php</string>
		</array>
		<key>RunAtLoad</key>
		<false/>
		<key>OnDemand</key>
		<true/>
		<key>StartCalendarInterval</key>
		<dict>
			<key>Hour</key>
			<integer>03</integer>
			<key>Minute</key>
			<integer>03</integer>
		</dict>
	</dict>
</plist>
<!--
	put this file into /Library/LaunchDaemons to load on boot
	you can also load it manually:
	launchctl load /x/y/de.bennigraf.backuprotation.plist
-->
